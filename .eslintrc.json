{
  "env": {
    "es2022": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }],
    "no-console": "off",
    "no-undef": "error",
    "no-dupe-else-if": "warn"
  },
  "ignorePatterns": [
    "node_modules/",
    ".env",
    "*.log"
  ]
}
